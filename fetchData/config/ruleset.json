        ["Shadow", "stat_variants"],
        ["Shadow", "Paldean_regional_subforms"],
        ["Shadow", "special_forms"],
        ["Shadow", "elemental_or_type_like"]
      ],


      "tracked_categories":["SPECIAL_FORM_WHITELIST"],

      "standalone_categories": ["SPECIAL_FORM_WHITELIST"]
    }
  }
  